{
  "expo": {
    "name": "SproutRoute",
    "slug": "sproutroute",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "sproutroute",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#E8F5E9"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sproutroute.app",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E8F5E9"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sproutroute.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-router", "expo-font"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7fa59f88-cc34-4884-b75c-9bcdc36e3d36"
      }
    },
    "owner": "nitishprasad"
  }
}
